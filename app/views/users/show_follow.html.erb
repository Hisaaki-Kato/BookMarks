<% provide(:title, @title) %>
<div class="container">
  <% if current_user?(@user) %>
    <h2><%= @title %> (<%= @count %>)</h2>
  <% else %>
    <h2><%= @user.name %>„ÅÆ<%= @title %> (<%= @count %>)</h2>
  <% end %>

  <% if @users.any? %>
    <ul class="users follow">
      <%= render @users %>
    </ul>
      <%= will_paginate %>
  <% end %>
</div>
